{
  "name": "policy-app",
  "version": "1.0.0",
  "description": "![coverage](https://img.shields.io/badge/coverage-4.5%25-red) ![release](https://img.shields.io/badge/release-1.2-blue) ![ghes](https://img.shields.io/badge/GHES_compliant-v3.5-darkgreen) ![ghes](https://img.shields.io/badge/GITHUB_compliant-true-orange) # Probot Policy App",
  "main": "src/index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "dev": "nodemon --exec \"npm start\"",
    "debug": "node --nolazy ./src/index.js",
    "start": "probot run ./src/index.js",
    "test": "jest",
    "quality": "npm run jslint",
    "test:watch": "npm run test -- --watchAll"
  },
  "jest": {
    "testRegex": "(test/.*(test)\\.js)",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/test/tmp/",
      "./src/eventHandlers/common/command.js"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 75
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jefeish/policy-app.git"
  },
  "keywords": [
    "policy"
  ],
  "author": "jefeish",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jefeish/policy-app/issues"
  },
  "homepage": "https://github.com/jefeish/policy-app#readme",
  "engines": {
    "node": ">= 10.13.0"
  },
  "dependencies": {
    "@octokit/auth-app": "^3.6.1",
    "@octokit/core": "^3.6.0",
    "@octokit/plugin-enterprise-server": "^10.6.4",
    "acorn": "^7.1.0",
    "bootstrap": "^3.4.1",
    "bootstrap-autocomplete": "2.3.7",
    "clone": "^2.1.2",
    "debug": "^4.1.1",
    "ejs": "^3.1.6",
    "events": "^3.2.0",
    "express-vue": "^5.15.0",
    "flat": "^5.0.0",
    "handlebars": "^4.7.7",
    "hash-it": "^4.0.5",
    "jquery": "^3.5.0",
    "js-yaml": "^3.14.0",
    "jshint": "^2.13.4",
    "jslint": "^0.12.1",
    "json-rules-engine": "^5.0.4",
    "jsonpath-plus": "^4.0.0",
    "lodash.isobjectlike": "^4.0.0",
    "nodemon": "^2.0.12",
    "probot": "^12.2.4",
    "probot-config": "^1.1.0",
    "serve-favicon": "^2.5.0",
    "swagger-ui-express": "^4.1.6",
    "yq": "^0.0.1"
  },
  "devDependencies": {
    "bootstrap": "^3.4.1",
    "bootstrap-autocomplete": "^2.3.7",
    "husky": "^7.0.4",
    "jest": "^26.4.0",
    "nock": "^13.0.4",
    "smee-client": "^1.2.2"
  }
}
